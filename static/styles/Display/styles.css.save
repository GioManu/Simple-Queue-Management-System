.blink {
    animation: blink 3s linear infinite;

}

@keyframes blink {
    20% {
        color: green;
    }

    40% {
        color: white;
    }

    60% {
        color: green;
    }

    80% {
        color: white;
    }

    100% {
        color: green;
    }
}

video {
a    position: absolute;
    top: 50%;
    left: 50%;
    min-width: 100%;
    min-height: 100%;
    width: auto;
    height: auto;
    z-index: 0;
    -ms-transform: translateX(-50%) translateY(-50%);
    -moz-transform: translateX(-50%) translateY(-50%);
    -webkit-transform: translateX(-50%) translateY(-50%);
    transform: translateX(-50%) translateY(-50%);
}

#MoveToDoneBtn {
    font-size: 150%;
    width: 150%;
}

.label {
    text-align: center;
    font-size: 350%;
}

.ordNum {
    /* float: left; */
    display: inline-block;
    width: 48%;
    font-size: 650%;
    color: white;
    text-align: center;

    border-style: solid;
    border-width: 1px;
    border-radius: 10%;

    margin-bottom: 2%;
    margin-right: 2%;
}

/* Split the screen in half */
.split {
    height: 100%;
    width: 50%;
    position: fixed;
    z-index: 1;
    top: 0;
    overflow-x: hidden;
}

/* Control the left side */
.left {
    left: 0;
    background-color: #111;
}

/* Control the right side */
.right {
    right: 0;
    background-color: #111;
}

/* If you want the content centered horizontally and vertically */
.centered {
    position: absolute;
    top: 50%;
    left: 35%;
    transform: translate(-50%, -50%);
    text-align: center;
}
